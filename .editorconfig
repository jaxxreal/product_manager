# top-most EditorConfig file
root = true

# 4 space indentation
[*.scss]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

[*.js]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

[*.rb]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true
